#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/nft/out/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/nft/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/nft/out/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/nft/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules/@vercel/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/@vercel+nft@0.22.6/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../@vercel/nft/out/cli.js" "$@"
fi
