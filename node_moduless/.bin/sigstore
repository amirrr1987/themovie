#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules/sigstore/bin/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules/sigstore/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules/sigstore/bin/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules/sigstore/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/sigstore@1.6.0/node_modules:/mnt/c/Users/amaghami/Documents/GitHub/themovie/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sigstore/bin/sigstore.js" "$@"
else
  exec node  "$basedir/../sigstore/bin/sigstore.js" "$@"
fi
